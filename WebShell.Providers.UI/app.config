<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="WebShell.Providers.UI.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <applicationSettings>
        <WebShell.Providers.UI.Properties.Settings>
            <setting name="Reg_Include" serializeAs="String">
                <value>(?i)\{%\s?include (\"[\w./]*\"|\'[\w./]*\')\s?%\}</value>
            </setting>
            <setting name="Reg_Block" serializeAs="String">
                <value>(?i)\{%\s?block \w*\s?%\}(.|\r\n)*\{%\s?endblock\s?%\}</value>
            </setting>
            <setting name="Reg_Extend" serializeAs="String">
                <value>(?i)\{%\s?extends \"[\w./]*\"\s?%\}</value>
            </setting>
            <setting name="Reg_FilePath" serializeAs="String">
                <value>\"[\w./]*\"|\'[\w./]*\'</value>
            </setting>
            <setting name="Reg_BlockStart" serializeAs="String">
                <value>(?i)\{%\s?block \w*\s?%\}</value>
            </setting>
            <setting name="Reg_BlockEnd" serializeAs="String">
                <value>{%\s?endblock\s?%\}</value>
            </setting>
            <setting name="Reg_BlockContent" serializeAs="String">
                <value>(.|\r\n)*?</value>
            </setting>
            <setting name="Reg_BlockName" serializeAs="String">
                <value>(?i)\{%|\s|block|%\}</value>
            </setting>
            <setting name="Reg_VarPattern" serializeAs="String">
                <value>\{\{\s?\w*\s?\}\}</value>
            </setting>
            <setting name="Reg_VarName" serializeAs="String">
                <value>[\{\s\}]*</value>
            </setting>
            <setting name="Reg_BlockValue" serializeAs="String">
                <value>((?i)\{%\s?block \w*\s?%\})|((?i)\{%\s?endblock\s?%\})</value>
            </setting>
            <setting name="Reg_BlockStartSearch" serializeAs="String">
                <value>(?&lt;=(?i)\{%\s?block \w*\s?%\})</value>
            </setting>
            <setting name="Reg_BlockEndSearch" serializeAs="String">
                <value>(?=(?i)\{%\s?endblock\s?%\})</value>
            </setting>
            <setting name="Reg_HTML_src_href" serializeAs="String">
                <value>\ssrc=\"[\w\./-]*\"\s?|\shref=\"[\w\./-]*\"\s?</value>
            </setting>
            <setting name="Reg_HTML_src_href_Replace" serializeAs="String">
                <value>href|src|[\"\s=]*</value>
            </setting>
        </WebShell.Providers.UI.Properties.Settings>
    </applicationSettings>
</configuration>